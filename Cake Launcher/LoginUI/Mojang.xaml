<Page x:Class="Cake_Launcher.LoginUI.Mojang"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cake_Launcher.LoginUI"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="200"
      Title="Mojang">

    <Grid>
        <TextBox x:Name="MojangEmail" Height="41" Width="200" Text="账号" FontSize="14" ToolTip="填写你的 Mojang 正版账户邮箱" Margin="0,113,0,46" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#CC0078D7" Foreground="White"/>
        <PasswordBox x:Name="MojangPassword" Height="41" Width="200" FontSize="14" ToolTip="填写你的 Mojang 正版账户密码" Margin="0,159,0,0" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#CC0078D7" Foreground="White"/>
    </Grid>
</Page>
